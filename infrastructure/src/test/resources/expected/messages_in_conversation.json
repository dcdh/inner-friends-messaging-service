{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "array",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": [],
  "examples": [
    [
      {
        "from": "Mario",
        "postedAt": "2021-10-01T00:00:00+02:00",
        "content": "Hello Peach !"
      }
    ]
  ],
  "additionalItems": true,
  "items": {
    "$id": "#/items",
    "anyOf": [
      {
        "$id": "#/items/anyOf/0",
        "type": "object",
        "title": "The first anyOf schema",
        "description": "An explanation about the purpose of this instance.",
        "default": {},
        "examples": [
          {
            "from": "Mario",
            "postedAt": "2021-10-01T00:00:00+02:00",
            "content": "Hello Peach !"
          }
        ],
        "required": [
          "from",
          "postedAt",
          "content"
        ],
        "properties": {
          "from": {
            "$id": "#/items/anyOf/0/properties/from",
            "type": "string",
            "title": "The from schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
              "Mario"
            ]
          },
          "postedAt": {
            "$id": "#/items/anyOf/0/properties/postedAt",
            "type": "string",
            "title": "The postedAt schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
              "2021-10-01T00:00:00+02:00"
            ]
          },
          "content": {
            "$id": "#/items/anyOf/0/properties/content",
            "type": "string",
            "title": "The content schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
              "Hello Peach !"
            ]
          }
        },
        "additionalProperties": false
      }
    ]
  }
}